    <!-- ANTI-RC -->
    <script src="js/right-click-prevent.js" async="async"></script>
    <script src="js/header-parallax.min.js" type="text/javascript"></script>
    <% if (title === "Contact") { %>
    <script src="js/material-input.min.js" type="text/javascript"></script>
    <script src="js/contact-page-validation.js" type="text/javascript"></script>
    <% } %>
  </body>
</html>

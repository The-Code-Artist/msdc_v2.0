<!DOCTYPE html>
<html lang="en-za">
  <!-- HEAD SECTION -->
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="google-site-verification" content="" />
    <title>
      <% if (title != "") { %> <%- title %> - MSDC Inc. <% } else { %> MSDC Inc. <% } %>
    </title>
    <link href="css/bootstrap.min.css" rel="stylesheet" />
    <link href="css/site.min.css" rel="stylesheet" />
    <!-- Parallax CSS -->
    <link rel="stylesheet" href="css/parallax.min.css" />
    <link rel="stylesheet" href="css/card.min.css" />
    <script src="https://use.fontawesome.com/66103fc3a5.js"></script>
    <link rel="shortcut icon" href="img/element.ico" />
    <% if (title === "Contact") { %>
    <link href="css/input.min.css" rel="stylesheet" />
    <style>
      #details {
        padding-top: 3.6em;
      }

      #loading {
        display: none;
        height: 16px;
        width: auto;
      }

      .alert {
        margin-top: 0.5rem;
      }

      #submit-btn {
        margin-bottom: 1rem;
      }

      @media screen and (max-width: 768px) {
        #details {
          padding-top: 0.4em;
          text-align: center;
        }
      }
    </style>
    <!--<script src="https://www.google.com/recaptcha/api.js?render=6LeBeN0ZAAAAAIpw4ooGeFNqB3gd58f_zLwqrKTR"></script>-->
    <% } %> <% if (title === "Terms") { %>
    <style>
      article p,
      ol {
        font-size: 13pt !important;
      }
    </style>
    <% } %> <% if (title === 'Portfolio') { %>
    <style>
      .proj {
        border-radius: 4px;
        padding: 1rem;
        transition: 0.6s ease;
      }

      .proj:hover {
        box-shadow: 0 3px 5px 0 rgba(0, 0, 0, 0.25);
        opacity: 0.8;
        transition: 0.4s ease;
      }
    </style>
    <% } %>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js"></script>
  </head>
  <!-- BODY -->
  <body>
    <!-- NAVBAR -->
    <section class="navbar navbar-default navbar-fixed-top">
      <!-- NAVBAR CONTAINER -->
      <section class="container">
        <section class="navbar-header">
          <button
            type="button"
            class="navbar-toggle"
            data-toggle="collapse"
            data-target=".navbar-collapse"
          >
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a href="/" class="navbar-brand">
            <img src="img/logos/logo.png" alt="MSDC Inc (Pty) Ltd" width="100" />
          </a>
        </section>
        <!-- COLLAPSABLE NAVBAR -->
        <section class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right list-unstyled">
            <li>
              <a href="./">Home</a>
            </li>
            <li>
              <a href="./services"> Services </a>
            </li>
            <li>
              <a href="./portfolio"> Portfolio </a>
            </li>
            <li>
              <a href="./contact"> Contact </a>
            </li>
          </ul>
        </section>
      </section>
    </section>

    <!-- SITE CONTENT -->
    <section id="body">
      <%- body %>
      <!-- BACK TO TOP BUTTON -->
      <a id="back-to-top" class="btn btn-danger back-to-top" role="button">
        <span class="fa fa-chevron-up"></span>
      </a>
    </section>
    <!-- FOOTER -->
    <footer class="footer-inverse">
      <section class="container-fluid text-center">
        <!-- SOCIAL ICONS -->
        <ul class="list-inline =" style="font-size: 15pt">
          <li>
            <a href="https://www.behance.net/MSDCInc" target="_blank" class="faa-shake animated"
              ><i class="fa fa-behance"></i
            ></a>
          </li>
          <li></li>
          <li>
            <a href="https://www.linkedin.com/company/18037125/" target="_blank"
              ><i class="fa fa-linkedin-square"></i
            ></a>
          </li>
          <li></li>
          <!--<li>
					<a href="https://www.facebook.com/MsdcInc/" target="_blank"><i class="fa fa-facebook-official"></i></a>
				</li>
				<li></li>-->
          <li>
            <a href="mailto:help.me@msdc.africa"><i class="fa fa-send"></i></a>
          </li>
        </ul>
        <!-- LINKS -->
        <p>
          <a href="./services">Services</a> | <a href="./terms">Terms</a> |
          <a href="./contact">Contact</a> |
          <a href="./portfolio">Portfolio</a>
        </p>
        <p>
          Developed By: <strong>MSDC Inc (Pty) Ltd &copy;</strong>
          <script>
                const year = new Date().getFullYear();
              document.write(' ' + year);
          </script> 
        </p>
      </section>
      <section class="footer-default container-fluid">
        <p class="text-center">
          <img src="img/MSFD-Element.png" style="max-width: 55px" />
        </p>
      </section>
    </footer>
    <!-- JQUERY CDN -->
    <script
      src="https://code.jquery.com/jquery-2.2.4.min.js"
      integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44="
      crossorigin="anonymous"
    ></script>
    <!-- JQUERY UI CDN -->
    <script
      src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"
      integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU="
      crossorigin="anonymous"
    ></script>
    <!-- BOOTSTRAP -->
    <script src="js/bootstrap.min.js" type="text/javascript"></script>

    <!-- BACK TO TOP SCRIPT -->
    <script type="text/javascript" async="async">
      $(document).ready(function () {
        $(window).scroll(function () {
          if ($(this).scrollTop() > 300) {
            $('#back-to-top').fadeIn();
          } else {
            $('#back-to-top').fadeOut();
          }
        });
        // TODO: SCROLL TO TOP OF PAGE WHEN CLICKED.
        $('#back-to-top').click(function () {
          $('body,html').animate(
            {
              scrollTop: 0,
            },
            2000
          );
          return false;
        });
      });
    </script>

